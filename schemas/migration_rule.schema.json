{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "CountryRule",
  "type": "object",
  "properties": {
    "country": {
      "type": "string",
      "enum": [
        "Canada",
        "USA",
        "Germany",
        "Netherlands",
        "Ireland",
        "Sweden",
        "Australia",
        "New Zealand"
      ]
    },
    "pathway": {
      "type": "string",
      "enum": ["Study", "Work", "PR"]
    },
    "min_ielts": { "type": "number", "minimum": 0, "maximum": 9 },
    "min_funds_usd": { "type": "number", "minimum": 0 },
    "min_degree": {
      "type": "string",
      "enum": ["high_school", "diploma", "bachelor", "master", "phd"]
    },
    "age_max": { "type": "integer", "minimum": 18, "maximum": 70 },
    "notes": { "type": "string" }
  },
  "required": ["country", "pathway"]
}
